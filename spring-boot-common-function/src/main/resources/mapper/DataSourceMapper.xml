<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="spring.boot.common.function.mapper.DataSourceMapper">

    <select id="getName" resultType="java.lang.String">
        select shangName from shangp where shangId = 1
    </select>

    <insert id="addShangp" parameterType="java.lang.String">
        insert into shangp(shangName,price) value (#{shangName},'2.34')
    </insert>

</mapper>







